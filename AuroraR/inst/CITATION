citHeader("To cite AuroraR in publications use:")

citEntry(
  entry    =  "Article",
  title    = "Empirical Bayes mean estimation with nonparametric errors via order statistic regression",
  author   =  personList( as.person("Nikolaos Ignatiadis"),
                              as.person("Sujayam Saha "),
                              as.person("Dennis L Sun"),
                              as.person("Omkar Muralidharan")),
  journal  = "arXiv",
  year     = 2019,
  url      = "https://arxiv.org/abs/1911.05970",
  textVersion = paste("Ignatiadis, N., Saha, S., Sun, D. L., & Muralidharan, O. (2019).",
                      "Empirical Bayes mean estimation with nonparametric errors via order statistic regression.",
                       "arXiv preprint arXiv:1911.05970.")
)



